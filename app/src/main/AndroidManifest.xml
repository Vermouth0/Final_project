<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swufe.wy.slimming">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />


    <application
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:allowBackup="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".FrameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PlanCheckActivity" />
        <activity android:name=".PlanEditActivity" />
        <activity android:name=".PlanEditActivityEdit" />
        <activity android:name=".PlanAddActivity" />
        <activity android:name=".PlanReportActivity" />
        <activity android:name=".FoodActivity" />
        <activity android:name=".PlayActivity"></activity>
    </application>

</manifest>